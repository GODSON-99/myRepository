import java.util.*;
import java.lang.*;
class Integers
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int k = sc.nextInt();
        int a = 0;
        for(int i =0;i<n;i++)
        {
            int num = sc.nextInt();
            if(num%k==0)
            {
                a=a+1;
            }
        }
        System.out.println(a);
    }
    
}
        
        
    
